{
    "name": "nicdev/ga-reporter",
    "description": "A PHP client for the Google Analytics API",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "google/apiclient": "^2.12",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6",
        "spatie/ray": "^1.41",
        "laravel/pint": "^1.23"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nicdev\\GoogleAnalytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nicdev\\GoogleAnalytics\\Tests\\": "tests/"
        }
    }
}